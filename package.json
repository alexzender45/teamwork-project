{
  "name": "teamwork-project",
  "version": "1.0.0",
  "description": "Teamwork-project",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel src -d build",
    "build": "npm run clean && npm run build-babel",
    "prestart": "npm run build && npm run create",
    "start": "node build/index.js",
    "pretest": "npm run build && npm run drop && npm run create",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --timeout 120000 --require @babel/polyfill tests --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "showcoverage": "start ./coverage/index.html",
    "lint": "eslint --ignore-path .gitignore .",
    "start1": "set DEBUG=http & nodemon --exec babel-node src/index.js",
    "create": "node build/model/ createTables",
    "drop": "node build/model dropTables"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzender45/teamwork-project.git"
  },
  "author": "Samuel Abiokorigho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexzender45/teamwork-project/issues"
  },
  "homepage": "https://github.com/alexzender45/teamwork-project#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/register": "^7.7.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.16.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "datauri": "^2.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.2.2",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "pg": "^6.4.2",
    "supertest": "^4.0.2"
  }
}